	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Current File (External Terminal)",
			"cwd": "/Volumes/data/Music/Artworks",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/env/bin/mitmproxy",
			"args": [
				"--set",
				"confdir=${workspaceFolder}",
				"-s",
				"${file}"
			],
			"console": "integratedTerminal"
		}
	]
}